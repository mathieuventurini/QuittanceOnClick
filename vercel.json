{
    "crons": [
        {
            "path": "/api/cron/send-receipt",
            "schedule": "*/30 * * * *"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}